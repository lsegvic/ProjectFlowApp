@model PracenjeTijekaProjekta.Models.PracenjeProjekta.UsersProject

@{
    ViewBag.Title = "View";
}

<h2>Project details</h2>
<link href="~/Content/deleteUsersProjects.css" rel="stylesheet" />
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt style="font-size:medium;margin-bottom:1%; margin-left:-6%">
            Student
        </dt>

        <dd style="margin-left:20%;margin-bottom:1%">
            @Html.DisplayFor(model => model.UsersNameSurname)
        </dd>

        <dt style="font-size:medium;margin-bottom:1%; margin-left:-6%">
            Project name
        </dt>

        <dd style="margin-left:20%;margin-bottom:1%" ">
            @Html.DisplayFor(model => model.ProjectName)
        </dd>

        <dt style="font-size:medium;margin-bottom:1%; margin-left:-6%">
            Project manager
        </dt>

        <dd style="margin-left:20%;margin-bottom:1%" ">
            @Html.DisplayFor(model => model.ProjectManager)
        </dd>

        <dt style="font-size:medium;margin-bottom:1%; margin-left:-6%">
            @Html.DisplayNameFor(model => model.Date)
        </dt>

        <dd style="margin-left:20%;margin-bottom:1%" ">
            @Html.DisplayFor(model => model.Date)
        </dd>

    </dl>
</div>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-actions " style="margin-left:-4%; font-size:medium">
        @Html.ActionLink("Back to List", "Index")
    </div>
}